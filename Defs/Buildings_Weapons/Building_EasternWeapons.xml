    <ThingDef ParentName="BuildingBase">
    <defName>OLL_Box_TT33M</defName>
    <label>Crate of TT-33</label>
    <description>A crate full of TT-33. Some TTs will become unusable when unbox.</description>
    <graphicData>
      <texPath>Things/Weapons/OLL_Box_TT33</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <altitudeLayer>Building</altitudeLayer>
    <rotatable>false</rotatable>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsExports</li>
    </thingCategories>
    <costList>
      <Steel>20</Steel>
      <OLL_TT33M>10</OLL_TT33M>
    </costList>
    <statBases>
      <SellPriceFactor>1.15</SellPriceFactor>
      <WorkToBuild>10000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>0</Flammability>
      <Mass>8</Mass>
    </statBases>
    <pathCost>50</pathCost>
    <passability>PassThroughOnly</passability>
    <size>(1,1)</size>
    <researchPrerequisites><li>BlowbackOperation</li></researchPrerequisites>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <designationCategory>Exportable</designationCategory>
    <tradeTags>
      <li>WeaponRanged</li>
    </tradeTags>
  </ThingDef>

